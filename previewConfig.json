{
  "projectRoot": ".",
  "componentPaths": [
    "src/components/**/*.{jsx,tsx,js,ts}",
    "app/**/*.{jsx,tsx,js,ts}"
  ],
  "extensions": [".jsx", ".tsx", ".js", ".ts"],
  "alias": {
    "@": "./src",
    "@components": "./src/components",
    "@assets": "./src/assets",
    "@hooks": "./src/hooks",
    "@utils": "./src/utils"
  },
  "framework": "next",
  "port": 3001,
  "css": {
    "tailwind": true,
    "configPath": "./tailwind.config.js"
  }
}
